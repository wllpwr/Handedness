<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.wllpwr.handedness">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Handedness"
        android:usesCleartextTraffic="true"
        tools:targetApi="31">
        <activity
            android:name=".Directions"
            android:exported="false"
            android:label="@string/title_activity_directions"
            android:theme="@style/Theme.Handedness" />
        <activity
            android:name=".BottomNavTest"
            android:exported="false"
            android:label="@string/title_activity_bottom_nav_test"
            android:theme="@style/Theme.Handedness" />
        <activity
            android:name=".MainMenu"
            android:exported="false"
            android:label="@string/title_activity_main_menu"
            android:theme="@style/Theme.Handedness" />
        <activity
            android:name=".MenuNavTest"
            android:exported="false"
            android:label="@string/title_activity_basic_menu_nav"
            android:theme="@style/Theme.Handedness" />
        <activity
            android:name=".MenuNavTest2"
            android:exported="false"
            android:label="@string/title_activity_basic_menu_nav"
            android:theme="@style/Theme.Handedness" />
        <activity
            android:name=".MenuNavTest3"
            android:exported="false"
            android:label="@string/title_activity_basic_menu_nav"
            android:theme="@style/Theme.Handedness" />
        <activity
            android:name=".BurgerTest"
            android:exported="false"
            android:label="@string/title_activity_burger_test"
            android:theme="@style/Theme.Handedness" />
        <activity
            android:name=".ConsentForm"
            android:exported="false"
            android:label="@string/title_activity_main_menu"
            android:theme="@style/Theme.Handedness" />
        <activity
            android:name=".SurveyForm"
            android:exported="false"
            android:label="@string/title_activity_main_menu"
            android:theme="@style/Theme.Handedness" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:theme="@style/Theme.Handedness">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>